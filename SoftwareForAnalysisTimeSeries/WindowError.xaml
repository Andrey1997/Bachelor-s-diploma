<Window x:Class="SoftwareForAnalysisTimeSeries.WindowError"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoftwareForAnalysisTimeSeries"
        mc:Ignorable="d"
        Title="Сообщение об ошибке" Height="145" Width="544" ResizeMode="NoResize" Background="#f7fffb" FontSize="16">
	<Grid>
		<TextBlock x:Name="TextBlockError" Text="Error" Margin="10,10,10,45"/>
		<Button Style="{StaticResource ButtonMainStyle}" x:Name="ButtonCloseWindow" Content="Ок" Margin="376,64,10,10" Click="ButtonCloseWindow_Click" RenderTransformOrigin="0.967,0.325" />
	</Grid>
</Window>
